@inproceedings{gomides_embriologia_2017,
	location = {Niterói},
	title = {Embriologia Humana na Wikipédia:  comparando acessos e edições de termos em português e inglês},
	url = {http://sgc.wikibrasil.org/index.php/iwsc/2017/schedConf/presentations},
	abstract = {Nos últimos anos, a internet tornou-se a principal fonte de informação em saúde, ultrapassando consultas a médicos e outros profissionais de saúde. A boa posição dos verbetes da Wikipédia nos mais importantes buscadores utilizados mostra o quão relevante a enciclopédia é para a obtenção de informação relacionada à saúde. Termos relacionados à embriologia humana são muito utilizados em sites de busca para a construção de conhecimento por estudantes da ampla área de biologia, e por indivíduos leigos em ciências da saúde, que procuram entender as etapas do desenvolvimento gestacional justificando assim a necessidade de um conteúdo completo e acurado nessa área. Este trabalho analisou e comparou dados sobre edição e acesso à quatro verbetes da Wikipédia-pt e -En relacionados à embriologia humana no período de agosto de 2016 a agosto de 2017. A análise dos dados mostrou que o número de edições e subtítulos dos verbetes na Wikipédia-En é muito superior aos da Wikipédia-Pt, demonstrando a necessidade de encorajar a participação dos editores da Wikipédia na língua portuguesa.},
	eventtitle = {I {IWSC}},
	author = {Gomides, Douglas and Soletti, Rossana and Guimarães, Maria Cristina},
